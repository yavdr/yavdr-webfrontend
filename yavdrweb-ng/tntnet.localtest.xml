<?xml version="1.0" encoding="UTF-8"?>
<tntnet>
  <mappings>
    <mapurl>
      <target>static@tntnet</target>
      <url>(.*)/$</url>
      <pathinfo>/$1/index.html</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/(.*)$</url>
      <pathinfo>/$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/icons/fugue/(.*)$</url>
      <pathinfo>/usr/share/icons/fugue/$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/icons/silk/(.*)$</url>
      <pathinfo>/usr/share/icons/silk/$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/icons/tango/(.*)$</url>
      <pathinfo>/usr/share/icons/Tango/$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/ext/(.*)$</url>
      <pathinfo>/usr/share/javascript/ext/$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/ext-ux/(.*)$</url>
      <pathinfo>/usr/share/doc/libjs-ext-examples/examples/ux/$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/$</url>
      <pathinfo>/tmp/yavdrweb/static/welcome/index.html</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/static/locale/(.*\.json)$</url>
      <pathinfo>/usr/share/yavdr/locale/$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>static@tntnet</target>
      <url>^/(.*\.(html|js|gif|png|css|ico|json))$</url>
      <pathinfo>/tmp/yavdrweb/$1</pathinfo>
    </mapurl>
    <mapurl>
      <target>redirect@tntnet</target>
      <url>^/admin$</url>
      <pathinfo>/admin/</pathinfo>
    </mapurl>
    <mapurl>
      <target>yavdrweb@yavdrweb</target>
      <url>^/admin/$</url>
      <pathinfo>/tmp/yavdrweb/templates</pathinfo>
    </mapurl>
    <mapurl>
      <target>$1@yavdrweb</target>
      <url>^/admin/([^/]*)$</url>
    </mapurl>
    <mapurl>
      <target>$2@$1</target>
      <url>^/(.*)/(.*)$</url>
    </mapurl>
  </mappings>
  <listeners>
    <listener>
      <ip></ip>
      <port>8000</port>
    </listener>
  </listeners>
  <daemon>0</daemon>
  <!--user>root</user>
  <group>root</group>
  <pidfile>/var/run/tntnet.pid</pidfile>
  <mimeDb>/etc/tntnet/mime.conf</mimeDb>
  <documentRoot>/tmp/yavdrweb</documentRoot-->
  <compPath>
    <entry>/usr/lib/tntnet</entry>
    <entry>/tmp/yavdrweb</entry>
  </compPath>
  <logging>
    <rootlogger>INFO</rootlogger>
    <loggers>
      <logger>
        <category>tntnet</category>
        <level>ERROR</level>
      </logger>
    </loggers>
    <file></file>
    <maxfilesize>1MB</maxfilesize>
    <maxbackupindex>2</maxbackupindex>
    <maxRequestSize>1048576</maxRequestSize>
    <!-- <host>localhost:1234</host> --> <!--  # send log-messages with udp -->
  </logging>
</tntnet>
